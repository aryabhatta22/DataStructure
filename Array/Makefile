.PHONY: clean All

All:
	@echo "----------Building project:[ ArrayOperations - Debug ]----------"
	@cd "ArrayAllOperations" && "$(MAKE)" -f  "ArrayOperations.mk"
clean:
	@echo "----------Cleaning project:[ ArrayOperations - Debug ]----------"
	@cd "ArrayAllOperations" && "$(MAKE)" -f  "ArrayOperations.mk" clean
